seen_numbers = []
final_list = []
print("Enter 10 numbers:")

for counter in range(1, 11):
    user_num = float(input(f"Input #{counter}: "))
    if user_num not in seen_numbers:
        seen_numbers.append(user_num)
        final_list.append(user_num)

print("\nFirst occurrences:")
print(" â†’ ".join(map(str, final_list)))
